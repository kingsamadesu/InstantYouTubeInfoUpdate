{
    "manifest_version": 2,
    "name": "Real Time YouTube Video Update",
    "version": "2.0",
    "permissions": [
      "activeTab",
      "tabs"
    ],
    "background": {
      "scripts":["background.js"]
    },  
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["main.js"]
        }
      ],
    "browser_action": {
      "default_icon":"res/img/LogoIcon24x24@2x.png",
      "default_title": "Real Time YouTube Video Update"
   }

  }